			<!--<tool-bar name="Catastro">
				<action-tool icon="" tooltip="Fusionar"
					action-command="_selection" position="200003"/>
			</tool-bar>-->
			<menu text="Catastro/Construcciones/Fusionar" action-command="_selection"
				icon="images/join.png" position="200006"/> 
		</extension>
		
		<extension class-name="es.icarto.gvsig.catastro.CutPolygonCatastroConstruccionExtension" description="Split Polygon" active="true">
			<!--<tool-bar name="Catastro">
